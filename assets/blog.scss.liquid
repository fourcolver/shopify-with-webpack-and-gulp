/* Components */
.blogs__heading {
  border-bottom: 4px solid #ed3fa8;
  padding-top: 3.125rem;
  padding-bottom: 1.875rem;
  margin-bottom: 1.875rem;
}

.blogs__heading h1 {
  color: #ed3fa8;
  text-align: center;
  text-transform: uppercase;
  font-weight: 700;
  margin: 0;
}

.blogs__featured a {
  display: block;
}

.blogs__featured-img {
  position: relative;
}

.blogs__featured-img img {
  width: 100%;
}

.blogs__featured-caption {
  padding: 2rem 1.25rem;
  bottom: 4.0625rem;
  position: absolute;
  left: 0;
  max-width: 45.3125rem;
}

.blogs__featured-caption:before {
  content: '';
  background-color: #92dff9;
  opacity: 0.8;
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.blogs__featured-caption p {
  font-size: 1.6875rem;
  font-weight: 600;
  text-transform: uppercase;
  color: #ed3fa8;
  margin-bottom: 0;
  font-style: italic;
}

.blogs__featured-text {
  position: relative;
}

.blogs__main {
  padding-bottom: 3.125rem;
}

.blogs__nav {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 1.5625rem 0;
  border-top: 1px solid #cbcbcb;
  border-bottom: 1px solid #cbcbcb;
  margin-bottom: 2.1875rem;
  margin-top: 1.5rem;
}

.blogs__nav-container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  width: 100%;
}

.blogs__nav-link {
  border-color: #ed3fa8;
}

.blogs__nav-link.active {
  color: #ed3fa8;
}

.blogs__nav-link:hover {
  -webkit-transition: border ease-in-out 400ms;
  transition: border ease-in-out 400ms;
  border-bottom: 1px solid #ed3fa8;
}

.blogs__nav-title {
  text-transform: uppercase;
  margin-bottom: 0;
  color: #575558;
  font-weight: 700;
}

.blogs .article {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.blogs .article:not(:first-child) {
  padding-top: 1.5625rem;
}

.blogs .article:not(:last-child) {
  padding-bottom: 1.5625rem;
  border-bottom: 1px solid #DCDCDC;
}

.blogs .article__img img {
  width: 100%;
}

.blogs .article__title {
  margin: 0;
  font-weight: 700;
  font-style: italic;
  color: #575558;
  padding-bottom: 1.25rem;
}

.blogs .article__text {
  font-style: italic;
  color: #575558;
  font-weight: 500;
}

