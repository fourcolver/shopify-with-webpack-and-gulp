///////////////////////////////////////////////////////////
*/
.pv__hdg {
  font-size: 1.75em;
  line-height: 1.35714em;
  margin: 0;
}

.fb-messenger-checkbox {
  display: none !important;
}

.license-tilt {
  -webkit-transform: rotate(-5deg);
          transform: rotate(-5deg);
}

/*
///////////////////////////////////////////////////////////
Essentials
///////////////////////////////////////////////////////////
*/

.pv-essentials {
  padding-top: 0.9375em;
  position: relative;
}

.pv-photos {
  z-index: 0;
}

.pv-actions {
  padding-top: 0.625rem;
}

.p-share {
  padding-top: 0.9375rem;
}

.pv-links {
  margin-top: 3.125rem;
}

.pv-links a {
  border-bottom: 1px solid #dfdfdf;
}

.pv-add-btn {
  font-size: 0.75rem;
  padding: 1.0625rem 0.625rem 1rem;
}

/*
///////////////////////////////////////////////////////////
Essentials
///////////////////////////////////////////////////////////
*/

.pv-desc {
  font-size: 0.875rem;
}

.pv-desc p {
  line-height: 1.6;
}

.pv-header {
  padding-bottom: 1.3125rem;
}

.pv-stock {
  font-size: 1.125em;
}

.pv-price {
  font-size: 1em;
  font-weight: 500;
  margin: 0;
}

.pv-price__compare-at {
  font-size: 0.9375rem;
}

.pv-title {
  color: #ed3fa8;
  font-size: 1.625rem;
  margin: 0 0 0.375rem;
  position: relative;
}

.pv-addon {
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 0.9375rem 0 1.25rem;
  width: 100%;
}

.pv-addon__bg {
  background-color: #f9f9f9;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-flex: 3;
  -webkit-flex-grow: 3;
          flex-grow: 3;
  min-height: 5rem;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding: 0.75rem 0.4375rem 0.75rem 1.875rem;
}

.pv-addon__bg h4 {
  font-size: 0.875rem;
  font-weight: 500;
  line-height: 1.125rem;
  margin: 0 0;
  padding: 0 0;
  text-align: left;
}

.pv-addon__bg h4 span {
  color: #ed3fa8;
  text-decoration: underline;
}

.pv-addon__bg label:before {
  background-color: #fff;
  border: 1px solid #dfdfdf;
  border-radius: 50%;
  /*styles outer circle*/
  content: " ";
  display: inline-block;
  height: 15px;
  margin: 0 7px 0 0;
  position: relative;
  top: 3px;
  width: 15px;
}

.pv-addon__bg label {
  color: #ed3fa8;
  font-size: 0.8125rem;
  margin-top: 0.4375rem;
  position: relative;
}

.pv-addon__bg label input[type='checkbox'] {
  display: none;
  /*removes original button*/
}

.pv-addon__bg label input[type='checkbox']:checked + span.pink {
  background-color: #ed3fa8;
  /*styles inside circle*/
  border-radius: 50%;
  display: block;
  height: 0.5625rem;
  left: 3px;
  padding-top: 6px;
  position: absolute;
  top: 6px;
  width: 0.5625rem;
}

.pv-addon__bg label span {
  font-size: 0.8125rem;
  font-weight: 500;
}

.pv-addon__bg--spooky-bag h4 {
  color: #ed3fa8;
}

.pv-addon__bg--spooky-bag p {
  font-size: 0.875rem;
  font-weight: 500;
  margin-bottom: 0;
}

.pv-addon__img {
  min-height: 5rem;
  width: 4.5rem;
}

/**
* gallery
*/
.pv-gallery {
  position: relative;
}

/**
* photos
*/
.pv-photos {
  width: 100%;
}

.pv-photos .pv-photo:not(.pv-photo-one) {
  width: 100%;
  text-align: center;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  top: 50%;
}

.pv-photos .pv-photo:not(.pv-photo-one):nth-child(even):before, .pv-photos .pv-photo:not(.pv-photo-one):nth-child(even):after {
  content: "";
  position: absolute;
  height: 2.25rem;
  left: 2.5rem;
  right: 2.5rem;
  background-color: #f8ebf5;
}

.pv-photos .pv-photo:not(.pv-photo-one):nth-child(even):before {
  top: -2.25rem;
}

.pv-photos .pv-photo:not(.pv-photo-one):nth-child(even):after {
  bottom: -2.25rem;
}

/**
* photo
*/
.pv-photo {
  width: 100%;
  text-align: center;
}

.pv-photo:nth-child(odd) .pv-photo__inner {
  padding: 0rem 2.5rem;
}

.pv-photo:nth-child(even) .pv-photo__inner {
  padding: 0rem 0rem;
}

/**
* thumbs
* (desktop and tablet only)
*/

.pv-thumbs__inner {
  z-index: 1;
}

.pv-thumbs__one-only {
  top: 25% !important;
}

/**
* thumb
*/
.pv-thumb {
  cursor: default;
  height: 2.25em;
  width: 2.75em;
  padding: 0 0.25rem;
}

.pv-thumb .ir {
  overflow: visible;
}

.pv-thumb__inner {
  border: 2px solid #fff;
  -webkit-transition: all 200ms ease-in-out;
  transition: all 200ms ease-in-out;
  border-radius: 100%;
}

.pv-thumb__img {
  border-radius: 100%;
}

.pv-thumb.is-selected .pv-thumb__inner {
  border-color: #f2b6ec;
}

.modal__product-photos {
  overflow: hidden;
  top: 0;
  z-index: 110;
  background-color: rgba(255, 255, 255, 0.75);
}

.modal__product-photos:before {
  content: none;
}

.modal__product-photos .modal__inner {
  border: none;
  max-width: 100%;
  padding: 0;
  background-color: transparent;
  height: 100%;
}

.modal__product-photos .modal__content {
  position: relative;
  height: 100%;
}

.pv-large-photos {
  max-width: 77.5rem;
  margin: 0 auto;
  padding: 0 6.5625rem;
}

.pv-large-photos .modal__close {
  top: 3.25rem;
  right: 2.5625rem;
}

.pv-large-photos .flickity-viewport {
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  top: 50%;
}

.pv-large-photos .flickity-prev-next-button {
  position: absolute;
  top: 50%;
  -webkit-transform: translate3d(0, -50%, 0);
          transform: translate3d(0, -50%, 0);
  padding: 0;
  margin: 0;
  background: 0;
  border: 0;
}

.pv-large-photos .flickity-prev-next-button:disabled {
  opacity: 0;
}

.pv-large-photos .flickity-prev-next-button svg {
  display: none;
}

