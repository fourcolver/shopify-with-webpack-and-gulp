  background-image: url(' {{ "sprite-icon-aeb32a2d.svg" | asset_url }} ');
  background-repeat: no-repeat;
  background-size: 25em 301.4375em;
  content: "";
  display: block;
  font-size: 1em;
}

.pv-large-photos .flickity-prev-next-button.previous {
  left: 1.25rem;
}

.pv-large-photos .flickity-prev-next-button.previous:before {
  background-position: 0em -43.6875em;
  width: 4.75em;
  height: 1.75em;
}

.pv-large-photos .flickity-prev-next-button.next {
  right: 1.25rem;
}

.pv-large-photos .flickity-prev-next-button.next:before {
  background-position: 0em -45.4375em;
  width: 4.75em;
  height: 1.75em;
}

.modal__product-photos .pv-large-photos {
  height: 100%;
}

.modal-overlay__large-photos {
  opacity: 0 !important;
}

